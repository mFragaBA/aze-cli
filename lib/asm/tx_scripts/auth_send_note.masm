    add.1
    push.253 exec.account::set_item
    dropw dropw
end

begin
    push.{recipient}
    push.{note_type}
    push.{tag}
    call.tx::create_note 
    drop drop dropw dropw
    call.tx_state_change dropw
    call.auth_tx::auth_tx_rpo_falcon512
    # => []
end
